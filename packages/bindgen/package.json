{
  "name": "near-bindgen-as",
  "version": "0.0.1-alpha.0",
  "description": "Bindings for serializing contract functions.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:as": "asc assembly/test.ts --transform ./src/index.ts --validate --runtime none --binaryFile out/test.wasm --textFile out/test.wat --runPasses inlining-optimizing,dce --debug",
    "test": "yarn build:as && yarn test:wasm",
    "test:ci": "yarn build:asconfig && yarn test",
    "test:wasm": "node ./test.js",
    "build:asconfig": "node asconfig.js"
  },
  "bin": {
    "near-asc": "./bin/near-asc"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nearprotocol/near-sdk-as.git",
    "directory": "packages/bindgen"
  },
  "keywords": [
    "near",
    "assemblyscript",
    "json",
    "blockchain",
    "crypto"
  ],
  "author": "Willem Wyndham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nearprotocol/near-sdk-as/issues"
  },
  "homepage": "https://github.com/nearprotocol/near-sdk-as/tree/master/packages/bindgen#readme",
  "devDependencies": {
    "near-runtime-ts": "file:../runtime",
    "ts-node": "^8.6.2",
    "typescript": "^3.6.4"
  },
  "files": [
    "bin/near-asc",
    "index.js",
    "assembly/index.ts",
    "compiler.js",
    "dist/"
  ],
  "peerDependencies": {
    "assemblyscript": "^0.9.2"
  },
  "dependencies": {
    "semver": "^7.1.3"
  },
  "private": true
}
